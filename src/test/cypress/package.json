{
  "name": "uds-identity-config",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cy.open.x509": "cypress open --config-file cypress.config.x509.ts",
    "cy.open.noX509": "cypress open --config-file cypress.config.noX509.ts",
    "cy.run.x509": "cypress run --config-file cypress.config.x509.ts",
    "cy.run.noX509": "cypress run --config-file cypress.config.noX509.ts",
    "cy.run": "npm run cy.run.x509 && npm run cy.run.noX509",
    "prettier": "prettier --write **/*.ts"
  },
  "devDependencies": {
    "cypress": "^13.7.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "dependencies": {
    "@zxing/browser": "^0.1.5",
    "otplib": "^12.0.1"
  }
}